{
    "name": "omaraligit/notoro",
    "description": "notoro is a php framework made easy for new comers to php and frameworks in general this project is open source and not production ready",
    "type": "project",
    "authors": [
        {
            "name": "omar ali",
            "email": "omarali0601520286@gmail.com"
        }
    ],
    "autoload":{
        "psr-4":{
            "App\\":"app",
            "Notoro\\":"src",
            "NotoroTest\\":"tests"
        },
        "files":[
            "src/framework/helpers/helpers.php"
        ]
    },
    "require": {
        "guzzlehttp/guzzle": "^6.3",
        "symfony/var-dumper": "^4.2",
        "http-interop/response-sender": "^1.0",
        "php-di/php-di": "^6.0",
        "vlucas/phpdotenv": "^3.1",
        "psr/http-server-middleware": "^1.0",
        "middlewares/whoops": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "scripts":{
        "server":"php -S localhost:8000 -t=public"
    }
}
